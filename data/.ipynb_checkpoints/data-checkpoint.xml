<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <data noupdate="1">
<!--        sequence for student -->
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">Student Sequence </field>
            <field name="code">school.student</field>
            <field name="prefix">SS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
       
           <record id="seq_school_appointment" model="ir.sequence">
            <field name="name">appointment Sequence </field>
            <field name="code">school.appointment</field>
            <field name="prefix">OT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

       </data>
</odoo>